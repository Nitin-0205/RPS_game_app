{
  "expo": {
    "name": "RPS",
    "slug": "nativeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Rock.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splas.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Rock.png",
        "backgroundColor": "#FFFFFF"
      },
      "package":"com.nitingupta.rps"
    },
    "web": {
      "favicon": "./assets/Rock.png"
    }
  }
}
